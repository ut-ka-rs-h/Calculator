pipeline {
    agent any
    stages {
        stage('Connected to Github') {
                steps {
                        echo 'Connected to github'
			
                }
        }
	    stage('Deploying'){
		    
		steps {
			echo 'Deploying...'
        }
	    }
        stage('Resources Allocation') {
                steps {
			echo "Hello"
                        }
        }
        stage('Final Stage') {
                parallel {
                        stage('Unit Test') {
                                steps{
                                        echo "Running the unit test..."
                                }
                        }
                        stage('Integration test') {
				steps {
					echo 'Running the integration test..'
				}
                               
			}  }
        }
    }
}
